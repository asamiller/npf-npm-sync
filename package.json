{
  "name": "npf-npm-sync",
  "version": "0.1.0",
  "description": "Sync the NPM registry's metadata with Orchestrate",
  "main": "index.js",
  "dependencies": {
    "async": "^0.9.0",
    "follow": "^0.11.2",
    "git-url-parse": "^4.0.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "npm-normalize": "^0.2.3",
    "octonode": "^0.7.1",
    "orchestrate": "^0.4.7",
    "q": "^1.4.1",
    "request": "^2.60.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "coveralls": "^2.11.1",
    "jscoverage": "^0.5.4",
    "jshint": "^2.5.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.1",
    "nock": "^2.10.0"
  },
  "scripts": {
    "test": "npm run jshint && npm run cov && npm run mocha",
    "mocha": "./node_modules/.bin/mocha test -R spec",
    "cov": "./node_modules/.bin/jscoverage lib lib-cov",
    "html-cov": "npm run cov && ./node_modules/.bin/mocha test -R html-cov > coverage.html",
    "coveralls": "npm run cov && ./node_modules/.bin/mocha test -R mocha-lcov-reporter | ./node_modules/.bin/coveralls",
    "jshint": "./node_modules/.bin/jshint bin lib test",
    "start": "node ./bin/index.js"
  },
  "bin": {
    "npf-npm-sync": "./bin/index.js"
  },
  "author": "Asa Miller <asa@asamiller.com>",
  "license": "MIT"
}
